@model IEnumerable<LetsRaid.Domain.Models.Guild>

@{
    ViewBag.Title = ViewBag.Server;
}

<h2>@ViewBag.Server</h2>

<table class="table">
    <tr>
        <th>
            Guilds
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "GetMembers", "Guilds", new { serverId = item.ServerId, guildId = item.GuildId, raidId = Request.QueryString["raidId"] }, null)
            </td>
        </tr>
    }

</table>
