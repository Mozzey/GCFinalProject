@model IEnumerable<LetsRaid.Models.GuildModels.Guild>

@{
    ViewBag.Title = ViewBag.Server;
}

<h2>@ViewBag.Server</h2>

<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.Server.Name)
        </th>*@
        <th>
            Guilds
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            @*<td>
                @Html.DisplayFor(modelItem => item.Server.Name)
            </td>*@
            <td>
                @Html.ActionLink(item.Name, "GetMembers", "Guilds", new { serverId = item.ServerId, guildId = item.GuildId }, null)
            </td>
        </tr>
    }

</table>
